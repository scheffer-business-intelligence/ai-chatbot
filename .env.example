# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=****

# Service account JSON used by Scheffer Agent Engine integration
# Relative paths are resolved from project root.
GOOGLE_SERVICE_ACCOUNT_KEY_FILE=bi-scheffer.json

# Google Cloud BigQuery persistence
BQ_PROJECT_ID=bi-scheffer
BQ_DATASET=scheffer_agente
BQ_AUTO_CREATE_TABLES=true
BQ_MESSAGES_TABLE=chat_messages
BQ_FILES_TABLE=chat_files

# Google Cloud Storage bucket for uploaded chat files
GCS_BUCKET_NAME=bi-scheffer-chat-files

# Direct provider API keys (used for GPT and Gemini models)
OPENAI_API_KEY=****
GOOGLE_GENERATIVE_AI_API_KEY=****

# The following keys below are automatically created and
# added to your environment when you deploy on Vercel

# Instructions to create an AI Gateway API key here: https://vercel.com/ai-gateway 
# Optional: required only if you want to use Vercel AI Gateway routes
# For Vercel deployments, OIDC tokens are used automatically
# https://vercel.com/ai-gateway
AI_GATEWAY_API_KEY=****

# Instructions to create a Redis store here:
# https://vercel.com/docs/redis
REDIS_URL=****

# Vertex AI Agent Engine (Scheffer ADK deployment)
VERTEX_PROJECT_ID=394738021127
VERTEX_LOCATION=us-central1
VERTEX_REASONING_ENGINE=projects/394738021127/locations/us-central1/reasoningEngines/9141406743538958336

OPENAI_REASONING_EFFORT=medium

# Max allowed attachment size (bytes) when sending inline_data to Agent Engine.
# Files above this limit return a clear error in chat.
AGENT_ENGINE_MAX_INLINE_FILE_BYTES=5242880
